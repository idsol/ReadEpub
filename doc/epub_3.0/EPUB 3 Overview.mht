/*    dl      *************************************************************=
*/
/**************************************************************************=
*/

dl dt {
    font-weight: bold;
}

dt span.term {
    color: #005A9C;
    font-weight: normal;
}

dd > dl {
    margin-top: 1px;
    padding-top: 0px;
    margin-bottom: 1px;
    padding-bottom: 0px;
}

/**************************************************************************=
*/
/*    ul      *************************************************************=
*/
/**************************************************************************=
*/

ul.conformance-list li {
    list-style-type: none;
}

